{
  "expo": {
    "name": "Compra Certa",
    "slug": "compra_rapida",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/cc.png",
    "scheme": "comprarapida",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "splash": {
      "image": "./assets/images/cc.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/cc.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.habby_valle_15.compra_rapida"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-sqlite"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "bf346491-18a0-4541-9e9a-d7eab0cbeb42"
      }
    }
  }
}
